    "next": "16.0.3",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.4.1",
    "stripe": "^19.3.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
