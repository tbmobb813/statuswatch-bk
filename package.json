    "@radix-ui/react-tabs": "^1.1.13",
    "@sendgrid/mail": "^8.1.6",
    "@stripe/stripe-js": "^8.4.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.81.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
